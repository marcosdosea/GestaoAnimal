@model Models.AplicaMedicamentoModel

@{
    ViewData["Title"] = "Consultar Aplicação";
}

<h1>Detalhes</h1>

<div>
    <h4>Aplicação de Medicamento</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Medicamento
        </dt>
        <dd class="col-sm-10">
            @ViewBag.Medicamento
        </dd>
        <dt class="col-sm-2">
            Animal
        </dt>
        <dd class="col-sm-10">
            @ViewBag.Animal
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Dosagem)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dosagem)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataAplicacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataAplicacao)
        </dd>
        <dt class="col-sm-2">
            Pessoa
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPessoa)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Observacoes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Observacoes)
        </dd>
